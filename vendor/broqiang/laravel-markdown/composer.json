{
    "name": "broqiang/laravel-markdown",
    "description": "Laravel markdown processing",
    "keywords": ["laravel", "markdown", "broqiang"],
    "license": "MIT",
    "authors": [
    {
        "name": "BroQiang",
        "email": "broqiang@qq.com"
    }],
    "require":
    {
        "php": "~7.0",
        "laravel/framework": "~5.5",
        "broqiang/laravel-image": "~1.0",
        "erusev/parsedown": "^1.7",
        "league/html-to-markdown": "^4.6",
        "mews/purifier": "^2.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "BroQiang\\LaravelMarkdown\\": "src/"
        },
